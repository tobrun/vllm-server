models:
  minimax-m2:
    script: /home/nurbot/ws/models/start_minimax_m2.sh

  glm45-air:
    script: /home/nurbot/ws/models/start_glm45_air.sh

  qwen3-coder:
    script: /home/nurbot/ws/models/start_qwen3_coder.sh
